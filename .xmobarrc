Config {
         font = "xft:inconsolata-15:antialias=true, xft:DejaVu Sans-10, xft:Noto Sans CJK SC:size=10:antialias=true"
       , bgColor = "#002b36"
       , fgColor = "#657b83"
       , position = Top
       , lowerOnStart = True
       , commands =
           [ Run MPD ["-t", "<state>: <artist> - <title>"] 10
           , Run Date "%a %d %b - %k:%M" "date" 600
           , Run Com "mpd_status.py" [] "mpd" 10
           , Run StdinReader
           , Run BatteryP ["BAT0"]
                 [ "--template", "<leftipat>"
                 , "--Low", "30" -- units: %
                 , "--low", "#dc322f"
                 , "--" -- battery specific options
                        -- discharging
                 , "--off-icon-pattern", "<fc=#b58900> ⚡ <left>% (<timeleft>) </fc>"
                 -- charging
                 , "--on-icon-pattern", "<fc=#859900> ⚡ <left>% </fc>"
                 -- charged
                 , "--idle-icon-pattern", "<fc=#859900> ⚡ </fc>"
                 ] 30
           ]
       , sepChar = "%"
       , alignSep = "}{"
       , template = "%StdinReader% }{<fc=#93a1a1>♫ %mpd%      </fc> %battery%       <fc=#93a1a1>%date%</fc>   "
       }
