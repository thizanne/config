Config {
         font = "xft:DejaVu Sans-8",
         bgColor = "white",
         fgColor = "black",
         position = Top,
         commands = [ 
          Run Date "%a %d %b - %k:%M" "date" 600,
          Run StdinReader,
	  Run BatteryP ["BAT0"] ["-t", "<left>%"] 10
	 ],
         sepChar = "%",
         alignSep = "}{",
         template = "%StdinReader% }{<fc=#a00>%battery%</fc> <fc=#ee9a00>%date%</fc>   "
       }
