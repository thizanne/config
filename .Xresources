Xft.antialias: true
Xft.hinting: true
Xft.rgba: rgb
Xft.hintstyle: hintfull
Xft.dpi: 116
Xft.lcdfilter: lcddefault

! urxvt
URxvt.buffered : true
URxvt.depth : 32
URxvt.font : xft:Inconsolata:size=15, xft:DejaVuSansMono:size=15, xft:Symbola:size=15
URxvt.letterSpace : -1
URxvt.scrollBar : false
!URxvt.transparent : true
!URxvt.shading : 20 ! transparent : 100; opaque : 0
!URxvt.tint : #002b36

! extensions
URxvt.perl-ext : default,url-select,selection-to-clipboard

!! url-select
URxvt.keysym.Mod4-u : perl:url-select:select_next
URxvt.url-select.launcher : xdg-open
URxvt.url-select.underline : true

URxvt.intensityStyles : false
Xcursor.theme: Vanilla-DMZ

